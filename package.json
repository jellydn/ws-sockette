{
  "name": "ws-sockette",
  "version": "0.0.1",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Huynh Duc Dung",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^16.11.26",
    "@types/ws": "^8.5.3",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.1",
    "tsm": "^2.2.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "uvu": "^0.5.3",
    "xo": "^0.48.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "build": "tsup --dts --format esm,cjs,iife --legacy-output",
    "dev": "DEBUG=* node -r tsm example/index.ts",
    "test": "xo && uvu -r tsm tests",
    "prepare": "husky install && yarn build"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write",
    "*.{js,ts}": "xo --fix"
  }
}
